{
  "version": "2.1.0",
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "raycast-departures",
  "title": "Norwegian Public Transport",
  "keywords": [
    "departures",
    "avganger",
    "norway",
    "norge",
    "public transport",
    "kollektivtransport",
    "travel",
    "reise",
    "entur",
    "journey planner",
    "buss",
    "tog"
  ],
  "description": "Norwegian public transport departures, based on real time data from Entur",
  "icon": "command-icon.png",
  "author": "rosvik",
  "owner": "variant-as",
  "categories": [
    "Web",
    "Data"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Departures",
      "subtitle": "View Public Transport Departures",
      "description": "Find departures from any stop place",
      "mode": "view",
      "arguments": []
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.68.1",
    "@raycast/utils": "^1.12.5",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.8",
    "@types/node": "20.11.20",
    "@types/react": "18.2.58",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
